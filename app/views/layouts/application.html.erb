<!DOCTYPE html>
<html>
<head>
  <title>Clubber</title>
  <%= stylesheet_link_tag :all %>
  <%= javascript_include_tag :defaults %>
  <%= csrf_meta_tag %>
</head>
<body>
<p class="notice"><%= notice %></p>
<p class="alert"><%= alert %></p>
<% if current_user %>
  <%= link_to 'Logout', :destroy_user_session %>
<% end %>
<%= yield %>

</body>
</html>
